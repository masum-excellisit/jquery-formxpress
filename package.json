{
  "name": "form-master",
  "version": "1.0.0",
  "description": "Universal jQuery Auto-Validation & Smart-Upload Plugin (FormMaster)",
  "keywords": [
    "jquery",
    "form",
    "validation",
    "upload",
    "plugin",
    "ajax",
    "forms"
  ],
  "author": "Masum",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masum-excellisit/form-master.git"
  },
  "bugs": {
    "url": "https://github.com/masum-excellisit/form-master/issues"
  },
  "homepage": "https://github.com/masum-excellisit/form-master#readme",
  "main": "dist/form-master.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "build": "mkdir -p dist && cp src/form-master.js dist/form-master.js && terser dist/form-master.js -o dist/form-master.min.js --compress --mangle --comments '/^!/'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test || echo 'No tests'"
  },
  "peerDependencies": {
    "jquery": ">=3.0.0"
  },
  "devDependencies": {
    "terser": "^5.24.0"
  }
}
